{
  "name": "telegram-bot",
  "version": "1.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm build:proto && tsc",
    "build:proto": "proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=src/proto proto/*.proto",
    "start": "node ./dist/index.js",
    "clean": "rm -rf dist; rm -rf src/proto",
    "clean:all": "rm -rf proto && pnpm clean",
    "format": "biome format --write src/",
    "lint": "biome lint --write src/",
    "check": "biome check --write src/",
    "prepare": "bash prepare.sh && husky",
    "docker": "docker build . -t telegram-bot"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "1.12.6",
    "@grpc/proto-loader": "0.7.15",
    "dotenv": "16.6.1",
    "kafkajs": "2.2.4",
    "logform": "^2.7.0",
    "postgres": "3.4.7",
    "telegraf": "4.16.3",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22.13.17",
    "grpc": "1.24.11",
    "grpc-tools": "1.13.0",
    "husky": "9.1.7",
    "protoc-gen-js": "3.21.4-4",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": "22.14.0",
    "pnpm": "10.6.5"
  },
  "volta": {
    "node": "22.14.0"
  },
  "packageManager": "pnpm@10.6.5"
}
